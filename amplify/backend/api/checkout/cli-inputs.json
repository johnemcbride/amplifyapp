{
  "version": 1,
  "paths": {
    "/checkout": {
      "name": "/checkout",
      "lambdaFunction": "checkout",
      "permissions": {
        "setting": "private",
        "auth": [
          "create",
          "read"
        ]
      }
    },
    "/checkout/response": {
      "name": "/checkout/response",
      "lambdaFunction": "stripeHandler",
      "permissions": {
        "setting": "open"
      }
    }
  }
}